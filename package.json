{
  "activationEvents": [
    "onCommand:markdown-auto-bullets.deleteLeft",
    "onDidChangeActiveTextEditor",
    "onDidOpenTextDocument",
    "onLanguage:markdown"
  ],
  "author": "Jason Lonsberry",
  "browser": "./src/extension.js",
  "categories": [
    "Formatters"
  ],
  "contributes": {
    "keybindings": [
      {
        "command": "markdown-auto-bullets.deleteLeft",
        "key": "backspace",
        "when": "editorTextFocus && editorLangId == markdown"
      }
    ]
  },
  "description": "Automatically add bullet points when editing in Markdown.",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@types/vscode": "^1.101.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.2",
    "eslint": "^9.37.0",
    "shx": "^0.4.0",
    "vscode-test": "^1.6.1"
  },
  "displayName": "Markdown Automatic Bullet Points",
  "engines": {
    "vscode": "^1.101.0"
  },
  "homepage": "https://github.com/transce080/vscode-markdown-auto-bullets",
  "keywords": [
    "auto",
    "automated",
    "bullets",
    "list",
    "md"
  ],
  "license": "AGPL-3.0-or-later",
  "main": "./src/extension.js",
  "name": "markdown-auto-bullets",
  "publisher": "jason-lonsberry",
  "repository": {
    "type": "git",
    "url": "https://github.com/transce080/vscode-markdown-auto-bullets"
  },
  "scripts": {
    "auth": "npx vsce login jason-lonsberry",
    "build": "shx mkdir -p build && npm run package && shx mv -f *.vsix build/",
    "lint": "eslint .",
    "package": "npx vsce package --no-dependencies",
    "pretest": "npm run lint",
    "publish": "npx vsce publish --no-dependencies",
    "test": "vscode-test"
  },
  "version": "1.2.1"
}
